generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

model Note {
  id        String  @id @default(uuid())
  name      String
  content   String
  isPublic  Boolean @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
